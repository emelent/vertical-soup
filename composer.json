{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "lukasoppermann/http-status": "^2.0",
        "codeception/codeception": "*",
        "flow/jsonpath": "^0.3.4",
        "kujira/phpunit-printer": "1.0.1",
        "limedeck/phpunit-detailed-printer": "^1.2",
        "tymon/jwt-auth": "1.0.0-beta.2",
        "neomerx/cors-illuminate": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9",
        "diablomedia/phpunit-pretty-printer": "^1.0",
        "whatthejeff/nyancat-phpunit-resultprinter": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
